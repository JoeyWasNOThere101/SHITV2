AutoPVPEnableOn.TextColor3 = Color3.fromRGB(255, 255, 255)
AutoPVPEnableOn.TextScaled = true
AutoPVPEnableOn.TextSize = 14.000
AutoPVPEnableOn.TextWrapped = true

AutoPVPEnableOn.MouseButton1Down:connect(function()
	AutoPVPEnableOn.Visible = false
	AutoPVPEnableOff.Visible = true
	_G.AutoEnablePVP = true
	while _G.AutoEnablePVP == true do
		wait()
		local Player = game.Players.LocalPlayer
		local Path = Player.PlayerGui.Main.Settings.PvpEnableButton
		for i,v in pairs(getconnections(Path.MouseButton1Click)) do
			v:Fire()
		end
	end
end)

