print("Made by Tamzid")


local uis = game:GetService("UserInputService")

uis.InputBegan:connect(function(inp)
	if inp.KeyCode == Enum.KeyCode.RightControl  then
		if Main.Visible then
			Main.Visible = not frame.Visible
		else
			Main.Visible = true
		end
	end
end)
